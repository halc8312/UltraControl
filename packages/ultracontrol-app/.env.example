# UltraControl Environment Variables Example
# Copy this file to .env.local and fill in your values

# LLM API Keys (At least one is required)
VITE_ANTHROPIC_API_KEY=your-anthropic-api-key-here
VITE_OPENAI_API_KEY=your-openai-api-key-here

# Optional LLM Providers
VITE_COHERE_API_KEY=
VITE_MISTRAL_API_KEY=
VITE_GROQ_API_KEY=
VITE_OLLAMA_BASE_URL=http://localhost:11434

# Default Models (Optional)
VITE_DEFAULT_ANTHROPIC_MODEL=claude-sonnet-4-20250514
VITE_DEFAULT_OPENAI_MODEL=gpt-4.1-mini

# Backend Services
VITE_OPENHANDS_URL=http://localhost:8000
VITE_DEVIN_API_URL=http://localhost:3001
VITE_BOLT_API_URL=http://localhost:5173

# WebSocket Configuration
VITE_WS_URL=ws://localhost:8000/ws/events
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10

# Plugin System
VITE_PLUGIN_DIR=./plugins
VITE_ENABLE_PLUGIN_SANDBOX=true
VITE_PLUGIN_AUTO_LOAD=false

# Community Features
VITE_TEMPLATE_API_URL=https://api.ultracontrol.dev/templates
VITE_BEST_PRACTICES_API_URL=https://api.ultracontrol.dev/practices

# Development Settings
VITE_DEBUG=false
VITE_LOG_LEVEL=info

# Feature Flags
VITE_ENABLE_VOICE_INPUT=true
VITE_ENABLE_REALTIME_COLLABORATION=false
VITE_ENABLE_AI_SUGGESTIONS=true

# Security
VITE_ENABLE_HTTPS=false
VITE_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173